{
  "name": "sportNews",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "test": "vue-cli-service build --env=test",
    "online": "vue-cli-service build  --env=online",
    "build": "vue-cli-service build --env=online",
    "report": "vue-cli-service build --env=test --report",
    "inspect": "vue-cli-service inspect --mode production >> webpack.config.production.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "jsonp": "^0.2.1",
    "md5": "^2.2.1",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "ftp-deploy": "^2.3.3",
    "html-loader": "^0.5.5",
    "node-sass": "^4.12.0",
    "opn": "^5.2.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "string-replace-loader": "^2.1.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-debugger": 1,
      "no-alert": 1,
      "no-console": 1,
      "consistent-return": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "iOS >= 8",
    "Firefox >= 20",
    "Android > 4.4"
  ]
}
